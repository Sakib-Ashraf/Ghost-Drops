  <p>{{section.settings.content}}</p>
    <img src="{{'SCN_Package_1.gif' | file_url}}" class="animte-top">
</section>
{% schema %}
  {
    "name": "Collectibles Top Section",
    "settings": [
	  {
        "id":"main_title",
        "type":"text",
        "label":"Main Page Title",

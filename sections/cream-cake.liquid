<section class=" product-detail">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-6 mob-view">
        <img src="{{section.settings.img_desktop | img_url:'680x915'}}" alt="" class="w-100" />
      </div>
      <div class="col-md-6">
        <div class="new prod-detail">
          <div class="scp-breadcrumb mt-0 mb-5">
            <ul class="breadcrumb wht-bdd mt-0">
              <li><a href="/pages/products">Products</a></li>
              <li class="active">{{ page.title }}</li>
            </ul>
          </div>
          <h2>{{section.settings.main_heading}}</h2>
          <ul class="new data-ratio">
            {{section.settings.content1}}
            {{section.settings.content2}}
            {{section.settings.content3}}
            {{section.settings.content4}}
            {{section.settings.content5}}
          </ul>
          <p>{{section.settings.content6}}</p>
          {{section.settings.content_all}}
          <hr class="mob-bd">
          <b class="pink">{{section.settings.available_heading}}</b>
          <ul class="funk-list clearfix">
            <li>
              <img src="{{section.settings.img_1 | img_url:'original'}}" alt="" />
              <h5>{{section.settings.gram1}}</h5>
            </li>
            <li>
              <img src="{{section.settings.img_2 | img_url:'original'}}" alt="" />
              <h5>{{section.settings.gram2}}</h5>
            </li>
            <li class="d-none">
              <img src="{{section.settings.img_3 | img_url:'original'}}" alt="" />
              <h5>{{section.settings.gram3}}</h5>
            </li>
          </ul>
          <hr class="mob-bd">
          <b class="pink">{{section.settings.lineage_heading}}</b>
          <ul class="tree-area  clearfix">
                     <li>
                        <div class="f-child">C.R.E.A.M Cake</div>
                        <ul class="branch-one z-sp-b-one clearfix">
                           <li>
                              <div class="new f-sub-child-one z-f-child" style="border: 2px solid #e31a77 !important;">Creamsicle</div>
                              
                           </li>
                           <li>
                              <div class="new f-sub-child-two z-sep">Pancakes</div>
                              
                           </li>
                        </ul>
                     </li>
                  </ul>
          
          <ul class="tree-area  clearfix d-none">
            <li>
              <div class="f-child">{{section.settings.main_heading}}</div>
              <ul class="branch-one clearfix">
                <li>
                  <div class="f-sub-child-one">{{section.settings.item1}}</div>
                  <ul class="branch-two">
                    <li>
                      <div class="second-sub-child-one">{{section.settings.item3}}</div>
                    </li>
                    <li>
                      <div class="second-sub-child-two">{{section.settings.item4}}</div>
                    </li>
                  </ul>
                </li>
                <li>
                  <div class="f-sub-child-two">{{section.settings.item5}}</div>
                  <ul class="marg-manage branch-two">
                    <li>
                      <div class="second-sub-child-one">{{section.settings.item2}}</div>
                    </li>
                    <li>
                      <div class="second-sub-child-two">{{section.settings.item6}}</div>
                    </li>
                  </ul>
                </li>
              </ul>
            </li>
          </ul>
          <hr class="mob-bd">
          <div class="partner-logos">
            <h2>{{section.settings.partner_heading}}</h2>
            <div class="logo-slider">
              <div>
                <a href="{{section.settings.logo1_link}}" target="_blank"> <img src="{{section.settings.logo1 | img_url:'original'}}"></a>
              </div>
              <div class="d-none">
                <a href="{{section.settings.logo2_link}}" target="_blank"><img src="{{section.settings.logo2 | img_url:'original'}}"></a>
              </div>
              <div class="d-none">
                <a href="{{section.settings.logo3_link}}" target="_blank"> <img src="{{section.settings.logo3 | img_url:'original'}}"></a>
              </div>
            </div>
          </div>
          <a href="{{section.settings.strain_link}}" class="all-strains">See all strains</a>
        </div>
      </div>
      <div class="col-md-6 dsk-view">
        <img src="{{section.settings.img_desktop | img_url:'680x915'}}" alt="" class="w-100 stick-img" />
       
      </div>
     
    </div>
    
  </div>
</section>


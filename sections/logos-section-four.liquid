<li class="logos-mobile" data-aos="fade-up-right"
    data-aos-delay="1"
    data-aos-duration="500"
    data-aos-easing="ease-in-out"
    data-aos-mirror="false"
    data-aos-offset="0"
    data-aos-once="false"
    data-aos-anchor-placement="top-center">
    <div class="circle-four position-relative">
      <img src="{{'cc-four.svg' | file_url}}" alt="" class="circ-four" />
      <h6>{{section.settings.heading}}</h6>
      <ul class="circle-list-four clearfix">
         {% for block in section.blocks %}
        <li><a href="{{block.settings.btn_link}}"><img src="{{block.settings.logo | img_url:'original'}}" alt="" /></a></li>
        {%endfor%}
      </ul>
    </div>
  </li>
<li class="logos-desktop">
    <div class="circle-four position-relative">
      <img src="{{'cc-four.svg' | file_url}}" alt="" class="circ-four" />
      <h6>{{section.settings.heading}}</h6>
      <ul class="circle-list-four clearfix">
         {% for block in section.blocks %}
        <li><a href="{{block.settings.btn_link}}"><img src="{{block.settings.logo | img_url:'original'}}" alt="" /></a></li>
        {%endfor%}
      </ul>
    </div>
  </li>
{% schema %}
  {
    "name": "Ghostfarm Circle 4",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading",
        "default": "LP / Distribution"
      }  
	],
    "blocks": [  
      {
      "type": "image",
      "name": "image",
      "settings": [
        
  		{
        "type": "image_picker",
        "id": "logo",
        "label": "Logos",
        "info": "680x 500px recommended"
        },
        {
        "type": "url",
        "id": "btn_link",
        "label": "Logo link"
      }
      ]
     }
    ],

    "presets": [{
      "name": "Ghostfarm Circle 4",
      "category": "HOMEPAGE"
    }]
  }
{% endschema %}